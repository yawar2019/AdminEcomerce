    <script>
    $(document).ready(function () {
      
        getCartCount();
    });
   
    function getCartCount() {
        var cart = new Object();
        cart.cust_id = 5;

        $.ajax({
            async: true,
            type: "POST",
            dataType: "JSON",
            contentType: 'application/json;',
            url: 'http://localhost:50235/Home/getCartCount',
            data: JSON.stringify(cart),

            processing: true,
            serverSide: true,
            success: function (data) {
                var record = JSON.parse(data.data);
                $('#CartCount').text(record);
            }
        }
   )
    };

  
    </script>

